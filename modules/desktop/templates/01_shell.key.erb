[org/gnome/shell]
<% if ! @dash_apps.empty? -%>
favorite-apps=[<% @dash_apps.each_index do |appidx| -%>
<% if appidx == 0 -%>
'<%= @dash_apps[appidx] %>'<% else -%>
,'<%= @dash_apps[appidx] %>'<% end -%>
<% end -%>
]
<% end -%>
<% if ! @extensions.empty? -%>
enabled-extensions=[<% @extensions.each_index do |extidx| -%>
<% if extidx == 0 -%>
'<%= @extensions[extidx] %>'<% else -%>
,'<%= @extensions[extidx] %>'<% end -%>
<% end -%>
]
<% end -%>
